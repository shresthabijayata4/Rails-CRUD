<%= link_to 'View all Journals', journals_path, class: "btn btn-primary"%><br><br>
<h1>Users</h1>
<%= link_to 'Add new user', new_user_path %><br/><br/>
    <% @users.each do |user| %>
        <label>First Name:</label>
        <%= user.first_name %><br/>
        <label>Last Name:</label>
        <%= user.last_name %><br/>
        <label>Email:</label>
        <%= user.email %><br>
        <%= link_to 'Show', user_path(user) %>
        <%= link_to 'Delete', user_path(user), method: :delete %>
        <br/><br/>

    <% end %>


