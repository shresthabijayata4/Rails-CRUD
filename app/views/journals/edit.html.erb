<%= form_for(@journal, method: :put) do |f| %>
    <%= f.label :Title %>
    <%= f.text_field :title %><br/><br/>
    <%= f.label :Amount %>
    <%= f.text_field :amount %><br/><br/>
    <%= f.label :CashDetails %>
    <%= f.text_field :cashin_out %><br/><br/>
    <%= f.submit 'Update User' %>
<% end %>